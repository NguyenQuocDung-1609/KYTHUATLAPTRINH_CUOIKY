point=input("Score:")
try:
    score=float(point)
except:
    print("Error, please enter numberic input")
if score < 0.0 or score > 1.0:
    print("Enter score:",score)
    print("Bad score")
elif score >= 0.9:
    print("Enter score:",score,"A" )
elif score >= 0.8:
    print("Enter score:",score,"B")
elif score >= 0.7:
    print("Enter score",score)
    print("C")
elif score >= 0.6:
    print("Enter score",score,)
    print("D")
else:
    print("Enter score",score)
    print("F")

